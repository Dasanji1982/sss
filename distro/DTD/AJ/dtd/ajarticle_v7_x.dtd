  				conflct 	(yes | no | decline | numerous | confidential) #REQUIRED>


<!--********************************************************************-->
<!-- End note                           			-->
<!--********************************************************************-->

<!ELEMENT 	endnote   			 			(p)+>

<!--********************************************************************-->
<!--       Citations                                      		-->
<!--********************************************************************-->

<!ELEMENT cits          		(cithead | citnote | citref)*  >
<!ATTLIST cits
	  style		(alpha | numero) "numero"				  >

<!-- cithead holds a heading for a section within cits. citnote holds paragraphed text within cits (but not including the actual references) if this is needed. citref holds an individual reference. -->

<!ELEMENT cithead     	    (%reftext; | %mathtypes;)*  >
<!ATTLIST cithead  	id		 ID #IMPLIED  >

<!ELEMENT citnote     	     (p+)  >

<!ELEMENT citref        	  		(citno?,(%citinfo;)+)                                  >
<!ATTLIST citref  id  		ID  	   #IMPLIED				  >

<!--     citno      	citation number                              -->
<!--     citaug      	cited author group                           -->
<!--********************************************************************-->
<!-- Cited author group elements are cittext plus the following 	-->
<!--     citau      	cited author                                      -->
<!--     citcorp      cited corporate author                           -->
<!--     citcollab    cited collaborators listed at end of author list  -->
<!-- Content allowed for cited author is cittext plus the following   -->
<!--     citsname     cited author surname                            -->
<!--     citfnames    cited author forenames                          -->
<!--     citsuff      cited author suffix                             -->

<!--********************************************************************-->

<!--     cityr      	cited year                                      -->
<!--     citart      cited article                                   -->
<!--     citso      	cited source                                    -->
<!--     citvol     	cited volume                                    -->
<!--     citsup     	cited supplement                                -->
<!--     citspn      cited start page number                         -->
<!--     citepn      cited end page number                         -->
<!--     citpub     	cited publisher                                 -->
<!--     citdoi         digital object identifier of the document    -->
<!--     cittext       free text additions -->
<!--     delim       punctuation -->


<!ELEMENT 	citno         	  			(%text;)*>
<!ELEMENT 	citaug         	  	( cittext | citau | citcorp | 
citcollab | delim)*  >
<!ELEMENT 	citau         	  	( cittext | citsname | citfnames | 
citsuff | delim)*  >
<!ELEMENT 	citcorp         	  			(%text; | delim)*>
<!ELEMENT 	citcollab       	  			(%text; | delim)*>
<!ELEMENT 	citsname        	  			(%text; | delim)*>
<!ELEMENT	citfnames       	  			(%text; | delim)*>
<!ELEMENT 	citsuff         	  			(%text; | delim)*>
<!ELEMENT 	cityr         	  			(%text;)*>
<!ELEMENT 	citart        	  			(%text;)*>
<!ELEMENT 	citso         	  			(%text;)*>
<!ELEMENT	citvol        	  			(%text;)*>
<!ELEMENT	citsup        	  			(%text;)*>
<!ELEMENT 	citspn         	  			(%text;)*>
<!ELEMENT 	citepn         	  			(%text;)*>
<!ELEMENT	citpub        	  			(%text;)*>
<!ELEMENT 	citdoi        	  			(#PCDATA)>
<!ELEMENT	cittext        	  			(%text; | %mathtypes;)*>
<!ELEMENT 	delim            	  			(%text;)* >

<!--********************************************************************-->
<!--       Appendix                                                 	-->
