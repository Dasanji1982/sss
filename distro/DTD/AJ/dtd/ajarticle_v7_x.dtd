<!--********************************************************************-->
<!--       Article Front Matter                               	-->
<!--********************************************************************-->

<!ELEMENT 	artfm 			  		(jti, sjti?, issn+, 
doi, altdoi*, vol?, iss?, issinfo?, isbn*, aopseq?, seq, pp, elocation?, serno?, pubdates, prdates?, chghst?, si?, seealso*, errorcor*, firstpagepdf?, no_pdf?, licence*, branding?)>

<!--			  		jti           	journal title
				  	sjti			journal short title
			       	issn          	issn
			       	doi           	digital object identifier
			       	vol           	volume number
		       		iss           	issue number
					issinfo       	extra issue information
					isbn		isbn
					aopseq	sequence number on AOP TOC
			  		seq		sequence of article in issue
			  		pp            	page numbers
			       	pubdates        	publication date
			       	prdates         	peer dates
			       	si		special issues and supplements
			       	artnote		general article note
			       	seealso			"see also" article link 
			       	no_pdf		No PDF placeholder tag 
			       	licence	A permanent flag in the XML to indicate it is freely accessible everywhere online;
			       					optional child element author-paid indicates an author has paid for the article
			       	branding	Additional branding besides Nature. E.g. the logo for the Montagna Symposium on some 
			       			JID articles.
			       			
			       			-->
<!ELEMENT 	jti   				  		(%text; | %mathtypes;)*>
<!ATTLIST		jti 
	language        %language;      #IMPLIED>

<!ELEMENT 	sjti   				  		(%text; | %mathtypes;)*>
<!ATTLIST		sjti 
		language        %language;      #IMPLIED>

<!ELEMENT 	issn							(#PCDATA)>
<!ATTLIST 	issn
				type	(print | online | prsup | olsup)   "print">
<!ELEMENT 	isbn							(#PCDATA)>
<!ATTLIST 	isbn
				type	(print | online | prsup | olsup)   "print"
				country	(us | uk | jp) #IMPLIED>

<!ELEMENT 	doi				  		(#PCDATA)>
<!ATTLIST 	doi 
			status	(reg | unreg | noreg)   "unreg"> <!-- DEPRECATED -->
<!-- Please note use of the "status" attribute is deprecated, it is no longer of practical use -->
<!-- Status of DOI is reg if DOI is registered with CrossRef, unreg if 
	to be registered but not yet registered, noreg if not to be registered. 
	Note that values have not been provided to distinguish between AOP 
	registration and later registration of final metadata, but these could 
	be added later if required. -->


<!-- HR: Alternative DOI element (for alternative DOIs). Requires unique LABEL attribute which establishes precedence. Zero or more allowed 
within ARTFM -->
<!ELEMENT 	altdoi       (#PCDATA)>
<!ATTLIST	altdoi     label  ID #REQUIRED>



<!ELEMENT 	vol							(#PCDATA)>
<!ELEMENT 	iss								(#PCDATA)>

<!ELEMENT 	issinfo					(covimg?) >
<!ELEMENT 	covimg			(%reftext; | %mathtypes;)* >
<!ATTLIST 	covimg        psn     (%psn;)   "external"
			      entname CDATA #IMPLIED
			      entref IDREF #IMPLIED
>				
<!--	covimg holds the text of a note on a cover image for the issue. 
	The default value of psn, external, refers to the standard method 
	of display of the cover image, placing the text of this note with 
	the image on a new HTML page linked to from the Web TOC. "entname"
	stores the graphic entity for the cover image. -->

<!ELEMENT 	aopseq						(#PCDATA)>
<!ELEMENT 	seq							(#PCDATA)>

<!ELEMENT 	pp    				  		(spn,epn)>

<!--    pp              	page numbers:
        spn                     start page
        epn                     end page                       		-->

<!ELEMENT 	spn 				  		(#PCDATA)>
<!ATTLIST 	spn 	tocshow 	(%tocshow;) 	"tocshow" >
<!ELEMENT 	epn 				  		(#PCDATA)>
<!ATTLIST 	epn 	tocshow 	(%tocshow;) 	"notoc" >

<!ELEMENT elocation (#PCDATA)>

<!ELEMENT	serno	(#PCDATA) >

<!ELEMENT 	pubdates					(aopdates, pbd, eld, cvd)>
				
<!--			aopdates	group to hold AOP dates 
			pbd		published date (print) 
			eld		electronic date (full issue) 
			cvd		cover date -->

<!ELEMENT 	aopdates			(aopd*, signoff?)>
<!ATTLIST 		aopdates
			pubol		(yes | no) "yes" >
<!-- NEW ELEMENT aopdates IN VERSION 6.0 -->
<!-- New element aopdates is provided as container for advance online publication date(s), held in elements 'aopd'. It is always present, whether or not any AOP is projected. Its attribute pubol has the value "yes" if AOP by NPG/Palgrave exists or is intended, "no" otherwise. If the value of pubol is "no" then aopdates is empty. --> 

<!ELEMENT 	aopd   				(%reftext;)*>
<!ATTLIST 		aopd 
			id		ID 		#IMPLIED 
			type   	(socms | pubol | author-ms)   #REQUIRED 
			year   	NMTOKEN  #REQUIRED 
			month  	NMTOKEN  #REQUIRED 
			day    	NMTOKEN  #REQUIRED>

<!-- NEW ELEMENT aopd IN VERSION 6.0 -->
<!-- New element aopd is provided for advance online publication date(s) 
	It can appear more than once because more than one type of online 
	publication is possible. Attribute type identifies the type of AOP. 
	type "socms" is publication of advance manuscript on a society web site.
	type "pubol" (the default) is advance publication on publisher's web site.
	Elements aopd are included only for existing or intended AOP publication. 
	If no AOP publication is projected, 'aopdates' is empty.

	Attribute id in aopd or in any pubdate element is for linking to the 
	element from dateref. These id attributes are new in version 6.0 
	and are #IMPLIED for backward compatibility with version 5.1
	
	Content of aopd can be a complete note about the AOP date, 
	to appear in PDF or HTML in position determined by processor, 
	or can be any text string required in addition to the standard 
	date conversion. -->
	
<!ELEMENT signoff EMPTY>

<!ATTLIST signoff
                  id ID #IMPLIED
                  year NMTOKEN #REQUIRED
                  month NMTOKEN #REQUIRED
                  day NMTOKEN #REQUIRED> 


<!ELEMENT 	pbd   		  		EMPTY>
<!ATTLIST 		pbd 
         id		   ID 	   #IMPLIED
			year   	NMTOKEN  #REQUIRED
         month  	NMTOKEN  #REQUIRED
         day    	NMTOKEN  #REQUIRED>

<!ELEMENT 	dateref        			  		(%text;)*>
<!ATTLIST 	dateref 		rid     	IDREF #REQUIRED>

<!ELEMENT 	eld   		  		EMPTY>
<!ATTLIST 		eld
			id 		ID 		#IMPLIED
 			year   	NMTOKEN  #REQUIRED
			month  	NMTOKEN  #REQUIRED
         day    	NMTOKEN  #REQUIRED >

<!ELEMENT 	cvd   	   		(#PCDATA)>
<!ATTLIST 		cvd
         id 		ID 		#IMPLIED
         year   	NMTOKEN  #IMPLIED
         month  	NMTOKEN  #IMPLIED
         day    	NMTOKEN  #IMPLIED >
<!-- Note that cover date as attributes is new in version 6.0. 
Although required everytime in new work, it has been made 
optional to preserve back compatibility. -->


<!--  The peer review date structure with four elements, rcdt, rvdt, 
	acdt and misct, new in DTD ver 6.0, can take content, and replaces 
	the former structure with elements rcd, rvd, acd, and misc. 
	The former structure is deprecated and retained for back 
	compatibility only. The new miscellaneous element misct can 
	hold a date and is repeatable. -->

<!ELEMENT 	prdates   		  		((rcd?, rvd*, acd?, misc?) | 
	(rcdt?, fddt?, rvdt*, acdt?, misct*))>

<!--		      		prdates         	peer-review dates: 
					rcd        received date (deprecated) 
					rvd        revised date (deprecated) 
					acd        accepted date (deprecated) 
					misc       Miscellaneous note (deprecated) -->

<!ELEMENT 	rcd  			  		EMPTY>
<!ATTLIST 		rcd 	year   	NMTOKEN  #REQUIRED 
               		month  	NMTOKEN  #REQUIRED 
                		day    	NMTOKEN #REQUIRED>
<!ELEMENT 	rvd  			  		EMPTY>
<!ATTLIST 		rvd 	year   	NMTOKEN  #REQUIRED 
               		month  	NMTOKEN  #REQUIRED 
                		day    	NMTOKEN  #REQUIRED>

<!ELEMENT 	acd  			  		EMPTY>
<!ATTLIST 		acd 	year   	NMTOKEN  #REQUIRED 
               		month  	NMTOKEN  #REQUIRED 
                		day    	NMTOKEN  #REQUIRED>

<!ELEMENT 	misc 				  		(%text;)*>

<!ELEMENT 	rcdt  		  		(%text;)*>
<!ATTLIST 		rcdt 	year   	NMTOKEN  #REQUIRED 
               		month  	NMTOKEN  #REQUIRED 
                		day    	NMTOKEN #REQUIRED>
<!ELEMENT 	rvdt  		  		(%text;)*>
<!ATTLIST 		rvdt 	year   	NMTOKEN  #REQUIRED 
               		month  	NMTOKEN  #REQUIRED 
                		day    	NMTOKEN  #REQUIRED>

<!ELEMENT 	fddt  		  		(%text;)*>
<!ATTLIST 		fddt 	year   	NMTOKEN  #REQUIRED 
               		month  	NMTOKEN  #REQUIRED 
                		day    	NMTOKEN  #REQUIRED>

<!ELEMENT 	acdt  		  		(%text;)*>
<!ATTLIST 		acdt 	year   	NMTOKEN  #REQUIRED 
               		month  	NMTOKEN  #REQUIRED 
                		day    	NMTOKEN  #REQUIRED>

<!ELEMENT 	misct 		  		(%text;)*>
<!ATTLIST 		misct year   	NMTOKEN  #IMPLIED 
               		month  	NMTOKEN  #IMPLIED 
                		day    	NMTOKEN  #IMPLIED>

<!ELEMENT chghst (chg)+>

<!ELEMENT chg (chgperson, chgdate, chgloc, chgmade, chgreason)>
