                           type              CDATA  #IMPLIED
                           entname           CDATA  #IMPLIED
                           entref            IDREF  #IMPLIED
>

<!--********************************************************************-->
<!-- LIST ELEMENTS      						-->
<!--********************************************************************-->

<!-- type elements are: -->
<!-- num:         1. 2. 3.       -->
<!-- numparen:    (1) (2) (3)    -->
<!-- lett:        a. b. c.       -->
<!-- lettparen:   (a) (b) (c)    -->
<!-- roman:       i. ii. iii.    -->
<!-- romanparen:  (i) (ii) (iii) -->
<!-- bullet:      bulleted       -->
<!-- none:        Unordered list -->

<!ELEMENT   	lst      			   		(lst | li)+>
<!ATTLIST   	lst      		id         	ID            #IMPLIED 
			hiding     (%hiding;)  "shown" 
        		type       	(num | numparen | lett | lettparen | 
				roman | romanparen | bullet | none) #REQUIRED>

<!ELEMENT  	li        	   	(%reftext; | %lst; | %mathtypes; | p)*>

<!--  Definition list                                             	-->
<!ELEMENT 	defl          		  		(definein,def)+>
<!-- "defl" -  character between definein and definition -->
<!--  "colwd" - relative column widths -->

<!ATTLIST 		defl			sepch 	CDATA   " "
          		colwd 	NMTOKENS "1 5" 
			hiding    (%hiding;)  "shown" >

<!ELEMENT definein      		  		(%reftext; | %mathtypes;)* >
<!ELEMENT def           			  	(%reftext; | %mathtypes;)* >

<!--********************************************************************-->
<!--  Math (AAP math elements have been removed in XML version of this DTD. 
	Elements removed are frac, numerator, denominator, stk, lyr, lim, op, 
	ul, ll, opd, prod, sum, integ, fen, cp, rp, fentext, rad, radicand, 
	radix. For tagged maths use altmath as a container, and tag 
	in MathML etc.)   -->
<!--********************************************************************-->

<!ELEMENT formula   			(%text; | %mathtypes;)*			  >
<!ATTLIST formula       id  		ID  #IMPLIED 
		hiding 	(%hiding;)  "shown" 
		psn 		(%psn;) "display" 
		format 	(%format;) "gif" 
		entname 	CDATA 	#IMPLIED
		entref  	IDREF 	#IMPLIED
		mathref	IDREFS 	#IMPLIED  > 

<!-- element altmath is container for maths tagging in MathML, TeX, LaTeX, HTML 
	or AAP -->
<!ELEMENT altmath 	   		(%reftext; | %mathtypes; | math)*>
<!ATTLIST    altmath 
	id 	ID #IMPLIED 
	type 	(mathml | tex | latex | html | aap) "mathml" >

<!--  subscripts and superscripts                                 	-->
<!ELEMENT sup              	(%reftext;)* 	       >
<!-- loc - location of superior -->
<!ATTLIST sup
          		loc   	(pre | post)   "post"                       >
<!ELEMENT inf              	(%reftext;)*     	  >
<!-- loc - location of inferior -->

<!ATTLIST inf
          		loc   	(pre | post)   "post"                       >

<!--********************************************************************-->
