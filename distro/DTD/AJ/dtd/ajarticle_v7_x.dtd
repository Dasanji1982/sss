	a similar summary to appear above the article, use inbrief or intro.  -->
<!--********************************************************************-->

<!ELEMENT 	tocnote			 			(p+)>

<!--********************************************************************-->
<!--     Series note. Note relating to series publication of the article. 
	This may include a chapter list and links to other articles using 
	extlink. See British Dental Journal 'practice' section for examples. -->
<!--********************************************************************-->

<!ELEMENT 	seriesnote			 			(p*)>

<!--********************************************************************-->
<!--     Article note                     -->
<!--********************************************************************-->

<!ELEMENT 	artnote			 			(p+)>

<!--********************************************************************-->
<!--       refjnl: caters for IAOR original article citation details    -->
<!--********************************************************************-->

<!ELEMENT	refjnl	(absjti, abssjti, absdoi, absaug, co?, absvol, absiss, abspp, abspbd)>

<!-- journal title of the original article -->
<!ELEMENT	absjti	(%text; | %mathtypes;)*>
<!ATTLIST	absjti	language	%language;	#IMPLIED>

<!-- short journal title of the original article -->
<!ELEMENT	abssjti	(%text; | %mathtypes;)*>
<!ATTLIST	abssjti	language	%language;	#IMPLIED>

<!-- DOI of the original article -->
<!ELEMENT	absdoi	(#PCDATA)>

<!-- Author group of the original article -->
<!ELEMENT	absaug	((absau | corpau | abscau)*, collab*, etal?, aff*, caff*, aunote*)>

<!-- single author of the original article -->
<!ELEMENT	absau	(%authors; | oref | autref)*>

<!-- corresponding author of the original article -->
<!ELEMENT	abscau	(%reftext; | coref | autref)*>

<!-- original article volume -->
<!ELEMENT	absvol	(#PCDATA)>

<!-- original article issue -->
<!ELEMENT	absiss	(#PCDATA)>

<!-- original article page span -->
<!ELEMENT	abspp	(spn, epn)>

<!-- original article publication date -->
<!ELEMENT	abspbd	(#PCDATA)>
<!ATTLIST	abspbd	id	ID	#IMPLIED
				year	NMTOKEN	#REQUIRED
				month	NMTOKEN	#REQUIRED
				day	NMTOKEN	#REQUIRED >



<!--********************************************************************-->
<!--       Header:	elements needed to complete the header    -->
<!--********************************************************************-->

<!ELEMENT	header			  (aopbiblio?, artbiblio?, 
	(abstract | kwg | edsumm | websumm | stndfrst)*, abbg?, xsum?)>
<!--********************************************************************-->
<!--Self-referring biblios. aopbiblio holds AOP reference to the article. 
	artbiblio holds final reference to the article. 	-->
