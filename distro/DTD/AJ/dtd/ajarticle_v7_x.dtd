<!ELEMENT 	aopbiblio 	 		(%reftext;)*>
<!ATTLIST 	aopbiblio  	id 	ID 	#IMPLIED>

<!ELEMENT 	artbiblio 	 		(%reftext;)*>
<!ATTLIST 	artbiblio  	id 	ID 	#IMPLIED>
	
<!--********************************************************************-->
<!--       Abstract                                               	-->
<!--********************************************************************-->
<!ELEMENT 	abstract  		  	((abshead?, (p | sec1)*), aopbiblio?, artbiblio?)>
<!-- Element abshead is reinstated. See documentation for preferred method 
	for abstract headings -->
<!ATTLIST 		abstract
language        %language;      #IMPLIED>

<!ELEMENT 	abshead		 			(%reftext; | %mathtypes;)* >

<!ELEMENT stndfrst (%reftext;)*>
<!-- A "standfirst" is an article summary, *not* a true abstract, that appears
 both in print and online. -->
 
<!--********************************************************************-->
<!--       Keyword  group                                               -->
<!--********************************************************************-->
<!ELEMENT 	kwg  				  		(kw+)  >
<!ATTLIST		kwg 	class  	CDATA		"keyword" 
				language        %language;      #IMPLIED>

<!-- in kwg use class attribute to show type of keywords, e.g. "kwd" if 
	ordinary keywords, "jel" if JEL classification, etc. For information, 
	the list of values recognised for this by the Elsevier DTD ver 4.3.1 
	is: kwd, abr, jel, msc, pacs, mat, src. idt, psycinfo, neuorsci, 
	inspec-cc, inspec-ct, inspec-chi, stma, astronomy, geo, cras-terre. -->


<!ELEMENT 	kw 				  		(%text; | %mathtypes;)*>

<!--********************************************************************-->
<!-- Editorial summaries (used for Graphical Abstracts in TOC -->
<!--********************************************************************-->

<!ELEMENT   edsumm                  (title?, p+)>
