<!ATTLIST 	sec3   hiding    (%hiding;)  "shown" >

<!-- Section Number -->
<!ELEMENT 	sec3ttl       		  (%reftext; | %mathtypes;)* >

<!--********************************************************************-->
<!--       Section Level 4                                        	-->
<!--********************************************************************-->

<!ELEMENT 	sec4          	  		(sec4ttl, (p | enunc | sec5)*)>

<!ATTLIST 	sec4   hiding    (%hiding;)  "shown" >

<!-- Section Number -->
<!ELEMENT 	sec4ttl       		  (%reftext; | %mathtypes;)* >

<!--********************************************************************-->
<!--       Section Level 5                                        	-->
<!--********************************************************************-->

<!ELEMENT 	sec5          	  		(sec5ttl, (p | enunc | sec6)*)>

