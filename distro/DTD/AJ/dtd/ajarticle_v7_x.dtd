<!ELEMENT 	app    				  		(apptl,appb)* >
<!ATTLIST	 	app    	id			ID		#REQUIRED>
<!-- 				apptl      		appendix title
				appb				appendix body-->

<!ELEMENT 	apptl   	 			(%reftext; | %mathtypes;)*>
<!ELEMENT 	appb   	  		((p | enunc)*, (sec1+)?)>

<!--********************************************************************-->
<!--       Elements that may be needed at the end of the article text -->
<!--********************************************************************-->

<!-- reader response number or other text asking for a response -->
<!ELEMENT 	respond  		 	(%reftext;)* >
<!ATTLIST 	respond 	resno 	CDATA 	#IMPLIED >

<!-- list of products mentioned in the text -->
<!ELEMENT 	prodlist  		 	(p*) >

<!-- closenote, used for any final note other than respond or prodlist -->
<!ELEMENT 	closenote   		  		(p*) >
<!ATTLIST 		closenote		id	ID		#IMPLIED >

