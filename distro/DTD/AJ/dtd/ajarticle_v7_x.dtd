					entref IDREF #IMPLIED
>

<!ELEMENT 	figttl        	 		(%text;)*>

<!ELEMENT 	legend        	  		(p)+>


<!--********************************************************************-->
<!--       ELEMENTS AND ATTRIBUTES REQUIRED FOR TABLE TAGGING -->
<!--  Definition of element table and other elements and attributes 
	required for tagging tables using our version of the CALS subset DTD 
	(which is fully incorporated in this document)	-->
<!--********************************************************************-->
<!ELEMENT table                               (%tabl;)   >
<!ATTLIST table                     tabstyle NMTOKEN #IMPLIED
                                tocentry %yesorno; "1"
                                    verified %yesorno; "0"
                                    shortentry %yesorno; #IMPLIED
                                    frame (top | bottom | topbot | all | sides | none)
                                      #IMPLIED
                                    colsep %yesorno; #IMPLIED
                                    rowsep %yesorno; #IMPLIED
                                    orient (port | land) #IMPLIED
                                    pgwide %yesorno; #IMPLIED
                                    %bodyatt;
                                tabcols CDATA #REQUIRED
                                    %secur; >

<!ELEMENT tbody                               (row+) >
<!ATTLIST tbody                     valign (top | middle | bottom) "top"
                                    %secur; >

<!ELEMENT tfoot                               (colspec*, row+)  >
<!ATTLIST tfoot                     valign (top | middle | bottom) "top"
                                    %secur; >

<!-- new element tabfn for a table footnote -->
<!ELEMENT 	tabfn   			  		(fnlink, fntext)>
<!ATTLIST 		tabfn  id   ID		#REQUIRED>

<!ELEMENT tgroup                     (colspec*, spanspec*, thead?, tfoot?, tbody, tabfn*) >

<!ATTLIST tgroup                     cols CDATA #REQUIRED
                                     tgroupstyle NMTOKEN #IMPLIED
                                     colsep %yesorno; #IMPLIED
                                     rowsep %yesorno; #IMPLIED
                                     align (%align;) "left"
                                     charoff CDATA "50"
                                     char CDATA ""
                                     show (yes | no) "yes" 
                                     %secur; >

<!ELEMENT thead                               (colspec*, row+)  >
<!ATTLIST thead                     valign (top | middle | bottom) "bottom"
                                    %secur; >

<!-- Note that entities % reftext and % mathtypes are defined in the 
	host DTD ajarticle version 6.0, and not within this fragment. 
	Noted 20 Aug 2002 by Tony Fowler, NPG. -->

<!ELEMENT title                       (%reftext; | %mathtypes;)* > 
<!ATTLIST title                     %secur; >

<!ELEMENT shorttl                       (%reftext; | %mathtypes;)* > 
<!ATTLIST shorttl                     %secur; >

<!ELEMENT colspec                               EMPTY >
<!ATTLIST colspec                     colnum CDATA #IMPLIED
                                      colname NMTOKEN #IMPLIED
                                      align (%align;) #IMPLIED
                                      charoff CDATA #IMPLIED
                                      char CDATA #IMPLIED
                                      colwidth CDATA #IMPLIED
                                      colsep %yesorno; #IMPLIED
                                      rowsep %yesorno; #IMPLIED >

<!ELEMENT spanspec                               EMPTY >
<!ATTLIST spanspec                     namest NMTOKEN #REQUIRED
                                       nameend NMTOKEN #REQUIRED
                                       spanname NMTOKEN #IMPLIED
                                       align (%align;) "center"
                                       charoff CDATA #IMPLIED
                                       char CDATA #IMPLIED
                                       colsep %yesorno; #IMPLIED
                                       rowsep %yesorno; #IMPLIED >

<!ELEMENT row                               ((entry | entrytbl)+) >
<!ATTLIST row                     rowsep %yesorno; #IMPLIED
                                  %secur; >

<!ELEMENT entry                               (%reftext; | %mathtypes;)* >
<!ATTLIST entry                     %entryatt;
                                    %secur; >

<!ELEMENT entrytbl                 (colspec*, spanspec*, thead?, tbody)+  >
<!ATTLIST entrytbl                     cols CDATA #REQUIRED
                                      tgroupstyle NMTOKEN #IMPLIED
                                       colname NMTOKEN #IMPLIED
                                       spanname NMTOKEN #IMPLIED
                                       colsep %yesorno; #IMPLIED
                                       rowsep %yesorno; #IMPLIED
                                       align (%align;) #IMPLIED
                                       charoff CDATA #IMPLIED
                                      char CDATA #IMPLIED
                                       %secur; >

<!ELEMENT tblimg                                  EMPTY>
<!ATTLIST tblimg                     format (%format;) "gif" 
                                     dpi       NMTOKEN #IMPLIED
                                     entname CDATA #IMPLIED
                                     entref IDREF #IMPLIED
                                     show (yes | no) "yes" >

<!--********************************************************************-->
<!--       ELEMENTS TO HOLD PULL QUOTES AND SIDE NOTES (MARGINAL NOTES)	-->
<!--********************************************************************-->

<!ELEMENT 	pullquote           		  		(%reftext;)*>
<!ATTLIST 	pullquote   hiding    (%hiding;)  "shown" 
			    id         	ID            #REQUIRED>
<!ELEMENT 	sidenote           		  		(%reftext;)*>
<!ATTLIST 	sidenote   hiding    (%hiding;)  "shown"
			   id         	ID            #REQUIRED>


<!--********************************************************************-->
<!--       ELEMENTS TO HOLD BOXED CONTENT				-->
<!--********************************************************************-->

<!ELEMENT 	box           		  		(boxttl?, p+)>
<!ATTLIST 	box   	    id         	ID            #REQUIRED>

<!ELEMENT 	boxttl           		  	(%text;)*>


<!--********************************************************************-->
<!--       ELEMENTS TO HOLD SCHEME CONTENT				-->
<!--********************************************************************-->

<!ELEMENT 	scheme       		  		(schemetl?, legend?)>
<!ATTLIST 	scheme          id            ID            #REQUIRED
                            format 	      (%format;)    "gif"
                            entname       CDATA         #IMPLIED
                            entref        IDREF         #IMPLIED
>

<!ELEMENT 	schemetl                           (%text;)*>



<!--********************************************************************-->
<!--       ELEMENTS FOR WEBLINKS SECTION				-->
<!--********************************************************************-->

<!ELEMENT linkgrp (linkset+)>

<!ELEMENT linkset (extlink+ | linksubset+)+>
<!ATTLIST linkset
name CDATA #IMPLIED
>

<!ELEMENT linksubset (extlink+)>

<!ATTLIST linksubset
name CDATA #IMPLIED
>

<!--********************************************************************-->
<!--       EXTERNAL OBJECTS                                      	-->
<!--********************************************************************-->

<!ELEMENT 	xobj           		  		(no?, xfile+, legend+)>
<!ATTLIST 		xobj     				id    		ID   #REQUIRED
			type 		CDATA              #IMPLIED>

<!ELEMENT 	xfile      	     	EMPTY>
