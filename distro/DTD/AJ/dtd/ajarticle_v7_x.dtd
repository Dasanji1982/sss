<!ELEMENT 	fnlink   			  		(%text;)*>
<!ELEMENT 	fntext   			  		(p+)>

<!ELEMENT 	appref        			  		(%text;)*>
<!ATTLIST 		appref	 rid    		IDREFS   #REQUIRED>

<!ELEMENT 	xheadref        			  		(%text;)*>
<!ATTLIST 		xheadref	 rid    		IDREFS   #REQUIRED>

<!ELEMENT 	xobref        			  		(%text;)*>
<!ATTLIST 		xobref	 rid    		IDREFS   #REQUIRED>

<!ELEMENT 	illref        			  		EMPTY >
<!ATTLIST 		illref	 rid    		IDREF   #REQUIRED>

<!ELEMENT	pullquoteref					EMPTY>
