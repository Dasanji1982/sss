<!ELEMENT 	tlnote   			 			(p+)>
<!ATTLIST 		tlnote			id     	ID #REQUIRED>

<!-- Review -->
<!ELEMENT 	review		(reviewtl?, hbiblio)>
<!-- Review Title -->
<!ELEMENT 	reviewtl		(%text;)*>
<!ATTLIST 	reviewtl 	tocshow 	(%tocshow;) 	"tocshow" >


<!--********************************************************************-->
<!--     Header biblio.  Usually biblio for a work reviewed, following the 
