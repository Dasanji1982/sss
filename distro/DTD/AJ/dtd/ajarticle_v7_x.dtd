<!-- Section Number -->
<!ATTLIST 	sec6   hiding    (%hiding;)  "shown" >

<!ELEMENT 	sec6ttl       		  (%reftext; | %mathtypes;)* >

<!--********************************************************************-->
<!--Note added in proof         					-->
<!--********************************************************************-->

<!ELEMENT 	prf  	 			(%reftext; | %mathtypes;)*>

<!--********************************************************************-->
<!-- Acknowledgements                           			-->
<!--********************************************************************-->

<!ELEMENT 	ack  				(p)+>

<!--********************************************************************-->
<!-- Author declarations                           			-->
<!--********************************************************************-->

<!ELEMENT   audecl 			(no?, p+)>
